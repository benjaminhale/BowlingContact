@model IEnumerable<Teams>
@ViewBag 

<a asp-action="Index" asp-controller="Home" asp-route-teamid="">Home</a>
<br />

<h1>Team Name</h1>

@foreach (var x in Model)
{
    <a class="btn btn-block @(x.TeamName == ViewBag.SelectedType ? "btn-primary" : "btn-outline-secondary")"
       asp-action="Index" asp-controller="Home" 
       asp-route-teamid="@x.TeamId"
       asp-route-team="@x.TeamName">
        @x.TeamName</a>
    <br />
}
